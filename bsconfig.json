{
  "name": "rescript-project-template",
  "version": "0.0.1",
  "sources": {
    "dir" : "src",
    "subdirs" : true
  },
  "package-specs": {
    "module": "es6-global",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [],
  "gentypeconfig": {
    "language": "typescript",
    "module": "es6",
    "generatedFileExtension": ".gen.ts",
    "debug": {
      "all": false,
      "basic": false
    },
    "shims": {
      "Webworker": "Webworker"
    }
  }
}
